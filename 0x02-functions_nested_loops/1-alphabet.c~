#include <stdio.h>
#include "holberton.h"
/**
 * main - program starts, evaluates and prints a string
 * separated by a comma and space
 * Return: Returns 0
 */
int _putchar(char c);

int main(void)
{
  _putchar('a');
  _putchar('b');
  _putchar('c');
  _putchar('d');
  _putchar('e');
  _putchar('f');
  _putchar('g');
  _putchar('h');
  _putchar('i');
  _putchar('j');
  _putchar('k');
  _putchar('l');
  _putchar('m');
  _putchar('n');
  _putchar('o');
  _putchar('p');
  _putchar('q');
  _putchar('r');
  _putchar('s');
  _putchar('t');
  _putchar('u');
  _putchar('v');
  _putchar('w');
  _putchar('x');
  _putchar('y');
  _putchar('z');
  _putchar('\n');
  return (0);
}


#include "holberton.h"
/**
 * print_alphabet - Print alphabet in lowercase using only `_putchar`
 * Return: 0
 */
void print_alphabet(void)
{
  char c = 'a';

  while (c <= 'z')
    {
      _putchar(c);
      c++;
    }
  _putchar('\n');
}
