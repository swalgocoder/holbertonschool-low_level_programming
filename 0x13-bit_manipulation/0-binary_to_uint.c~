#include "holberton.h"
#include <stdio.h>
#include <string.h>
/**
 *binary_to_uint - converts str to int
 *@b: ptr to stg
 *Return: unsigned int
 */

unsigned int binary_to_uint(const char *b)
{

	int dec = 0, i, len;


	len = strlen(b);
	for(i=0; i<len; i++)
	{
		dec = dec * 10 + ( b[i] - '0' );
	}
	return(dec);
}
